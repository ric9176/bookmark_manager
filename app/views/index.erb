<h1> Links </h1>

<ul id='links'>
  <% @links.each do |link| %>
    <li>
      Title: <%= link.title %>
      URL: <a href="<%= link.url %>"><%= link.url %></a>
      TAGS: <%= link.tags.map(&:name).join(' ') %>
    </li>
  <% end %>
</ul>

<form action="/links/new">
 <input type="submit" value="Add Link">
</form>

